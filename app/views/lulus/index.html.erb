<p id="notice"><%= notice %></p>

<h1>Lulus</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lulus.each do |lulu| %>
      <tr>
        <td><%= lulu.title %></td>
        <td><%= lulu.description %></td>
        <td><%= link_to 'Show', lulu %></td>
        <td><%= link_to 'Edit', edit_lulu_path(lulu) %></td>
        <td><%= link_to 'Destroy', lulu, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lulu', new_lulu_path %>
